{
    "_description": "APRICOT object detection, contributed by MITRE Corporation",
    "adhoc": null,
    "attack": {
        "knowledge": "white",
        "kwargs": {
            "batch_size": 1,
            "split": "frcnn"
        },
        "name": "apricot_dev",
        "type": "preloaded",
        "use_label": false
    },
    "dataset": {
        "test": {
            "batch_size": 1,
            "name": "apricot_dev",
            "split": "frcnn"
        }
    },
    "defense": {
        "kwargs": {
            "apply_fit": false,
            "apply_predict": true,
            "clip_values": [
                0.0,
                1.0
            ],
            "quality": 10
