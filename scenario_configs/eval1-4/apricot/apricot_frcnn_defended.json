    },
    "model": {
        "fit": false,
        "fit_kwargs": {},
        "model_kwargs": {},
        "module": "armory.baseline_models.pytorch.mscoco_frcnn",
        "name": "get_art_model",
        "weights_file": null,
        "wrapper_kwargs": {}
    },
    "scenario": {
        "kwargs": {},
        "module": "armory.scenarios.object_detection",
        "name": "ObjectDetectionTask"
    },
    "sysconfig": {
        "docker_image": "twosixarmory/armory",
        "external_github_repo": null,
        "gpus": "all",
        "output_dir": null,
        "output_filename": null,
        "use_gpu": false
    }
